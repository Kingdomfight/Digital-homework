if {[is_project_open]} {
	if {[string compare $quartus(project) "Design_Assignment_2"]} {
		puts "Project Design_Assignment_2 is not open"
		set make_assignments 0
	}
} else {
	# Only open if not already open
	if {[project_exists Design_Assignment_2]} {
		project_open -revision Design_Assignment_2 Design_Assignment_2
	} else {
		project_new -revision Design_Assignment_2 Design_Assignment_2
	}
	set need_to_close_project 1
}

# Make assignments
